<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<fx:root alignment="TOP_RIGHT" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefWidth="400.0" type="javafx.scene.layout.HBox" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
    <children>
      <TextArea fx:id="textArea" editable="false" maxHeight="1.7976931348623157E308" minHeight="-Infinity" prefWidth="300.0" wrapText="true">
         <cursor>
            <Cursor fx:constant="DEFAULT" />
         </cursor>
         <font>
            <Font size="11.0" />
         </font>
         <HBox.margin>
            <Insets />
         </HBox.margin></TextArea>
      <VBox maxHeight="1.7976931348623157E308" minHeight="-Infinity" prefHeight="100.0" prefWidth="100.0">
         <children>
            <Label fx:id="sender" text="Label" textAlignment="CENTER">
               <font>
                  <Font name="Comic Sans MS Bold" size="18.0" />
               </font>
               <padding>
                  <Insets left="15.0" />
               </padding>
            </Label>
              <ImageView fx:id="displayPicture" fitHeight="99.0" fitWidth="99.0" pickOnBounds="true" preserveRatio="true" />
         </children>
      </VBox>
    </children>
    <padding>
        <Insets bottom="15.0" left="10.0" right="10.0" top="15.0" />
    </padding>
</fx:root>
